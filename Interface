

public interface LL {

	public class Node{
		
		private String name;
		private int age;
		private int pinCode;
		Node next;
		
		public Node(String name,int age,int pinCode) {
			this.name = name;
			this.age = age;
			this.pinCode = pinCode;
			next = null;
		}
		
		public String getName() {
			return this.name;
		}
		
		public int getAge() {
			return this.age;
		}
		
		public int getPincode() {
			return this.pinCode;
		}
		
		public String getDisplay() {
			return "The name is "+this.name + " Age : "+this.age + " and stays at "+this.pinCode;
		}
	}

}
