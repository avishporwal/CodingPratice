

public class PriorityQueue  {
	
	static class Node{
		String data;
		int priority;
		Node next;
	}
	
	
	static Node newNode(String d, int p) {
		Node temp = new Node();
		temp.data = d;
		temp.priority = p;
		temp.next = null;
		return temp;
		
	}
	
	static String peek(Node head) {
		return head.data;
	}
	
	static Node pop(Node head) {
		head = head.next;
		return head;
	}
	
	static Node push(Node head, String d, int p) {
		Node start = head;		
		Node temp = newNode(d,p);		
		if(start.priority < p) {			
			temp.next = head;
			head = temp;
		}
		else {			
			while( start.next!=null && start.next.priority >= p) {				
				start = start.next;				
			}			
			temp.next = start.next;
			start.next = temp;	
		}	
		return head;
	}
	
	static int isEmpty(Node head) {
		return head == null ? 1:0;
	}

	public static void main(String[] args) {
		
		Node priorityQueue = newNode("love",5);   
		priorityQueue = push(priorityQueue,"!",0);
		priorityQueue = push(priorityQueue,"my",1);
		priorityQueue = push(priorityQueue,"I",8);
		priorityQueue = push(priorityQueue,"country",1);
		
		while(isEmpty(priorityQueue)==0) {
			System.out.println(peek(priorityQueue));
			priorityQueue = pop(priorityQueue);
		}
		

	}

}
